.graph-filter .info
  c $text
line[stroke="#000"]
  v 0 $base7
text
  &[fill="#000"], &[fill="#6a737d"], &[text-anchor="middle"]
    v $text
.viz
  .code-frequency
    ../, &
      c $text $base1 $base4
    p 8px
.c
  &add
    c $green
  &del
    c $red
.num, relative-time
  c $faded
.graphs
  &.axis
    c 0 $base1
  .dir
    c $text
  .area
    v $teal
.code-frequency
  .addition
    v $green
  .deletion
    v $red
.community-checklist .progress
  c 0 0 $base8
table.capped-list
  td
    c $text $base1 $base4
  tr:nth-child(2n), th
    c $text $base1 $base4
.summary-stats li
  &, & > .num, & > a
    ease background-color
  a
    ease background-color, color
  &:hover, &:focus
    &, & > .num, & > a
      c $cn $base1 $base5
  /.traffic-graph-stats, &, & > .num
    c $text $base1 $base4
.pulse-section
  c $faded
.contrib-person path
  v $orange
#contributors-master svg.viz
  c 0 0 $base4
.rule
  bb $b
