.task-progress
  c $faded
  .progress-bar .progress
    c 0 0 $region
.compare-pr-placeholder
  c 0 $border $hl
  p
    c $text
.comment-reactions.has-reactions
  c 0 $border $alt
.range-editor
  c 0 $border $alt
  &-icon
    c $faded
.select-menu
  &-tabs
    c 0 $border
    ../-tab-nav
      &.selected, &[aria-selected="true"], a.selected, a[aria-selected="true"]
        bc $border $border $hl
        c $cn 0 $hl
.blob-code-
  &addition
    c 0 0 $tealTrans
  &deletion
    c 0 0 $redTrans
.Progress
  c 0 0 $icon
.github-pr-file span
  &.deletion
    c 0 0 $red
  &.addition
    c 0 0 $green
.SelectMenu-
  &blankslate, &loading, &message
    c $text $border $alt
  &item
    ease background-color
    &:hover
      c 0 0 $hl
.status-meta,
.merge-pr-more-commits
  c $faded
ul.comparison-list
  c 0 $border $alt
  & > li
    c $text $border $alt
    em
      c $faded
  & > li.title
    c $text $border $hl
.branch-
  &action-state-clean
    ../action-body::before
      brc $hl
  &action-item-icon.completeness-indicator
    .octicon
      v $border
    &-success
      c $border 0 $green
    &-error
      c $border 0 $red
  &action-state-clean &action-icon
    .octicon
      v $border
  &infobar
    c 0 $border $alt
  &action-body
    b 0
    c 0 $border $hl
    caret "right"
    .merge-
      &branch-form, &message
        c 0 $border $alt
.merge-status-
  &list
    c 0 $border
  &item
    c 0 $border $alt
.timeline-comment
  c $text $border $hl
  /:target &
    s 0 0 0 0.2em $border
    &::before
      filter drop-shadow(-0.28em 0 0 $border)
  &-wrapper
    b 0
    c 0 $border
  &-label.tooltipped.tooltipped-s.tooltipped-multiline
    c $purple $purple $purpleTrans
  &-header
    c $cn $border $alt
  &--caret
    &, &.current-user
      &::before, &::after
        brc $alt
.composer.composer-responsive .timeline-comment
  caret "right"
.tabnav-pr .tabnav-tab.js-pjax-history-navigate.selected
  c 0 0 $bg
.review-
  &thread-reply
    c 0 $border $alt
    .inline-comment-form
      c 0 0 $hl
  &comment
    c $text
