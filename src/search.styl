.filter-bar
  bb $b
  c 0 0 $alt
.form-control, .form-select
  s none
.form-control.search-page-label.js-advanced-search-label
  &, & input
    c 0 0 $hl
.code-list
  .file-box, & &-item + &-item, .divider .blob-code, .divider .blob-num, &-item-private .blob-num
    c $text $border $hl
  &-item
    &.hx_hit-code&-public.repo-specific
      c 0 0 $bg
.jump-to-field-active, .js-navigation-item:hover, #jump-to-results:hover
  c 0 0 $hl
.jump-to-suggestions
  &-results-container .navigation-focus &-path
    c 0 0 $region
#commit_search_results .commit
  bx $b
  c $text $border $alt
  px 8px
.form-control.focus, .focus.ais-SearchBox-input, .form-control:focus, .ais-SearchBox-input:focus, .form-select.focus, .form-select:focus
  c 0 $blue
  s none
.ais-
  /.search-result-intro
    & mark, & .ais-Highlight-highlighted
      bbc $blue
  &Hits-item
    &:hover
      c 0 0 $icon
  &SearchBox-input
    bg $alt url("/assets/images/octicons/search.svg") no-repeat 6px
    c $cn $border
header #search-results-container
  c 0 0 $alt
