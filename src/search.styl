.filter-bar
  bb $b
  c 0 0 $base4
header #search-results-container
  c 0 0 $base4
.search_repos
  fieldset
    c 0 0 $base3
    px 16px
.user, .issue
  {adjacent('&-list-item')}
    btc $base1
.code-list
  .file-box, & &-item + &-item, .divider .blob-code, .divider .blob-num, &-item-private .blob-num
    c $text $base1 $base5
  &-item
    &.hx_hit-code&-public.repo-specific
      c 0 0 $base2
.js-navigation-open
  ease color
.jump-to-
  &field-active, .js-navigation-item:hover, #jump-to-results:hover
    c 0 0 $base5
  &suggestions
    &-results-container .navigation-
      &item
        c 0 0 $base4
      &focus &-path
        c 0 0 $base8
.ais-
  /.search-result-intro
    & mark, & .ais-Highlight-highlighted
      bbc $blue
  &Hits-item
    &:hover
      c 0 0 $base7
  &SearchBox-input
    bg $base4 url("/assets/images/octicons/search.svg") no-repeat 6px
    c $cn $base1
#commit_search_results .commit
  bx $b
  c $text $base1 $base4
  px 8px
