.task-progress
  c $faded
  .progress-bar .progress
    c 0 0 $base8
.compare-pr-placeholder
  c 0 $base1 $base5
  p
    c $text
.comment-reactions
  .user-has-reacted
    c 0 $base1 $base5
  &.has-reactions
    c 0 $base1 $base4
.range-editor
  c 0 $base1 $base4
  &-icon
    c $faded
.select-menu
  &-tabs
    c 0 $base1
    ../-tab-nav
      &.selected, &[aria-selected="true"], a.selected, a[aria-selected="true"]
        bc $base1 $base1 $base5
        c $cn 0 $base5
.blob-code-
  &addition
    c 0 0 $tealTrans
  &deletion
    c 0 0 $redTrans
.Progress
  c 0 0 $base7
.github-pr-file span
  &.deletion
    c 0 0 $red
  &.addition
    c 0 0 $green
.SelectMenu-
  &blankslate, &loading, &message
    c $text $base1 $base4
  &item
    ease background-color
    &:hover
      c 0 0 $base5
.status-meta,
.merge-pr-more-commits
  c $faded
ul.comparison-list
  c 0 $base1 $base4
  & > li
    c $text $base1 $base4
    em
      c $faded
  & > li.title
    c $text $base1 $base5
.branch-
  &action-
    &state-clean
      ../body::before
        brc $base5
    &item-icon.completeness-indicator
      .octicon
        v $base1
      &-success
        c $base1 0 $green
      &-error
        c $base1 0 $red
    &state-clean &action-icon
      c 0 0 $teal
      .octicon
        v $base1
  &body
    b 0
    c 0 $base1 $base5
    caret "right"
    .merge-
      &branch-form, &message
        c 0 $base1 $base4
  &infobar
    c 0 $base1 $base4
.merge-
  &branch-description
    c $faded
  &status
    &list
      c 0 $base1
    &item
      c 0 $base1 $base4
.timeline-comment
  c $text $base1 $base5
  /:target &
    s 0 0 0 0.2em $base1
    &::before
      filter drop-shadow(-0.28em 0 0 $base1)
  &-wrapper
    b 0
    c 0 $base1
  &-label.tooltipped.tooltipped-s.tooltipped-multiline
    c $purple $purple $purpleTrans
  &-header
    c $cn $base1 $base4
  &--caret
    &, &.current-user
      &::before, &::after
        brc $base4
.composer.composer-responsive .timeline-comment
  caret "right"
.tabnav
  &-pr &-tab.selected
    c 0 0 $base2
.review-
  &thread-reply
    c 0 $base1 $base4
    .inline-comment-form
      c 0 0 $base5
  &comment
    c $text
