.compare-pr-placeholder
  c 0 $border $hl
  p
    c $text
.range-editor
  c 0 $border $alt
  &-icon
    c $faded
.select-menu-tabs
  c 0 $border
  .select-menu-tab-nav.selected,
  .select-menu-tab-nav[aria-selected='true'],
  a.selected,
  a[aria-selected='true']
    bc $border $border $hl
    c $cn 0 $hl
.github-pr-file span
  &.deletion
    c 0 0 $red
  &.addition
    c 0 0 $green
.SelectMenu
  &-blankslate,
  &-loading,
  &-message
    c $text $border $alt
ul.comparison-list
  c 0 $border $alt
  & > li
    c $text $border $alt
    em
      c $faded
  & > li.title
    c $text $border $hl
