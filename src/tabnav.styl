.tabnav
  bb $b
  /.container-preview &-tabs
    ml 5%
  &-extra, &-tab
    c $faded
  &-pr &-tab.selected
    c 0 0 $base2
  &-tabs.js-file-editor-nav &-tab
    c $faded $base1 $base2
    mt 8px
    rt 6px
  &--responsive &-tab, &-tab
    c $faded $base1
    &[aria-current], &[aria-selected=true], &.selected
      c $text $base1 $base5
      &.js-blob-edit-
        &code, &preview
          c $blue $base1 $base5
