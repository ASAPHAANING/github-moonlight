@require "variables"
@require "mixins"

.js-gist-file-update-container.js-task-list-container.file-box
  c 0 0 $hl
.cm-s-github-light .CodeMirror-linenumber
  c $indigo
#gist-pjax-container
  c 0 0 $alt
.application-main
  mb 16px
.cm-
  &attribute
    c $yellow
  &string
    c $green
  &operator
    c $cyan
.form-select,
input[type="text"].form-control
  c $text $border $hl
.gisthead .pagehead-actions .octicon-mute
  v $faded
.timeline-comment-wrapper.timeline-new-comment.js-comment-container.width-fit, .discussion-timeline-actions, .reponav.js-repo-nav, .reponav-item
  &, &.selected
    c 0 0 $alt
