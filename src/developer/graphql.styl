iframe#graphiql
  &, .graphiql-ide
    c 0 $base1
.graphiql-ide
  btc $base1
.CodeMirror
  &-cursor
    blc $blue
  &-hint
    c $text $base1 $base4
    ease background-color
    &-information
      c $indigo $base1 $base4
    &-active
      c $cn 0 $base8
      fw 500
  &-linenumber
    c $indigo
    fw 500
  &-gutters, &-gutter&-foldgutter
    c $text $base1 $base4
  &.cm-s-graphiql
    c 0 0 $base4
.history-, .doc-explorer-
  &contents
    c $text $base1 $base2
  &title-bar
    c $text $base1 $base4
.graphiql-container
  .graphiql-container .history-contents p
    c $text $base1
  .execute-options > li,  .toolbar-menu-items > li, .toolbar-select-options > li, .history-contents > p
    &:hover, &.hover, &:active
      c 0 $base1 $base8
  .search-box
    c 0 $base1 $base2
    input
      c 0 0 $base2
  .result-window .CodeMirror-gutters
    c 0 $base1
  .doc-category-title
    c $faded $base1
  .resultWrap
    blc $base1
  .arg-name
    c $pink
  .field-name
    c $blue
  .keyword
    c $purple
  .type-name
    c $yellow
  .field-short-description
    c $indigo
  .topBar
    bg $base4
    c $text $base1 $base4
  .toolbar, .execute
    &-button
      bg $base2
      s none
      svg
        v $text
  .variable-editor-title
    c $text $base1 $base5
  .docExplorerShow
    bg $base5
    c $text $base1 $base5
.result-window .CodeMirror
  c 0 0 $base4
.cm-
  &punctuation
    c $text
  &comment
    c $indigo
  &def
    c $blue
  &property
    c $teal
  &string, &string-2
    c $green
  &keyword
    c $purple
